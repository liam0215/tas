#!/usr/bin/gnuplot -persist
set terminal pdf
set output "plot.pdf"
set colorsequence podo
set key autotitle columnhead
set xlabel "# Connections"
set ylabel "Latency (Âµs)"
set key left top

# Plot 50p latency
plot 'lat_50p.dat' using 1:2:xtic(1) title 'bare-tas' linetype 2 w lp, \
     'lat_50p.dat' using 1:3:xtic(1) title 'bare-vtas' linetype 3 w lp, \
     'lat_50p.dat' using 1:4:xtic(1) title 'virt-tas' linetype 4 w lp, \

